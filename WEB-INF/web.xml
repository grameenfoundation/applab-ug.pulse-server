<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
   version="2.5">
   
   <servlet>
    <servlet-name>SubmitSupportTicket</servlet-name>
    <servlet-class>applab.pulse.server.SubmitSupportTicket</servlet-class>
   </servlet>
   
   <servlet>
    <servlet-name>GetTabs</servlet-name>
    <servlet-class>applab.pulse.server.GetTabs</servlet-class>
   </servlet>
   
   <servlet-mapping>
	<servlet-name>SubmitSupportTicket</servlet-name>
	<url-pattern>/submitSupportTicket</url-pattern>
   </servlet-mapping>
   
   <servlet-mapping>
	<servlet-name>GetTabs</servlet-name>
	<url-pattern>/getTabs</url-pattern>
   </servlet-mapping>
 </web-app>
